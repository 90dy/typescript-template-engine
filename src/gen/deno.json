{
  "name": "@tmpl/gen",
  "description": "Code generation CLI for template literals",
  "author": "90dy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/90dy/typescript-template-engine"
  },
  "publish": {
    "include": [
      "**/*.ts",
      "README.md",
      "deno.json",
      "bin/**/*"
    ],
    "exclude": [
      "**/*.test.ts"
    ]
  },
  "exports": {
    ".": "./runner.ts",
    "./main.ts": "./main.ts"
  },
  "tasks": {
    "build": "deno run -A build.ts",
    "build:all": "deno task build"
  },
  "bin": {
    "tmpl-gen": "./runner.ts"
  },
  "version": "0.6.3"
}
